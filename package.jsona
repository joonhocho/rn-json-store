{
  "name": "auth0-react-native",
  "version": "0.0.2",
  "description": "React Native port of Auth0 SDK (auth0/auth0.js)",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "BABEL_ENV=babel babel src --out-dir lib",
    "build-watch": "BABEL_ENV=babel babel src --watch --out-dir lib",
    "start": "npm run build && npm run test-build && npm run test",
    "test": "mocha testout/**/*.js",
    "test-build": "BABEL_ENV=babel babel test --out-dir testout",
    "test-build-watch": "BABEL_ENV=babel babel test --watch --out-dir testout",
    "test-watch": "mocha --watch testout/**/*.js",
    "watch": "npm run build-watch & npm run test-build-watch & npm run test-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonhocho/auth0-react-native.git"
  },
  "keywords": [
    "react-native",
    "react",
    "native",
    "auth0",
    "sdk"
  ],
  "author": "Joon Ho Cho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joonhocho/auth0-react-native/issues"
  },
  "homepage": "https://github.com/joonhocho/auth0-react-native#readme",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "node-fetch": "^1.5.1",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "abab": "^1.0.3",
    "babel-runtime": "^6.6.1",
    "jwt-node-decoder": "0.0.3",
    "qs": "^6.1.0"
  }
}
